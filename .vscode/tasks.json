{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: cl.exe 生成活动文件",
			"command": "cl.exe",
			"args": [
				"/Zi",
				"/EHsc",
				"/nologo",
				"/MD",
				"/Fe:",
				"${fileDirname}/out/${fileBasenameNoExtension}.exe",
				"${file}",
				"/I",
				"C:/L/C++/vcpkg/installed/x64-windows-static-md/include",
				"/link",
				"/LIBPATH:C:/L/C++/vcpkg/installed/x64-windows-static-md/lib"
			],
			"options": {
				"cwd": "${fileDirname}/out"
			},
			"problemMatcher": [
				"$msCompile"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "编译器: cl.exe"
		}
	]
}